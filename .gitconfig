[push]
    default = simple
[core]
    editor = vim
[alias]
    a = add
    b = branch
    ci = commit
    cia = commit -am
    cl = clone
    co = checkout
    cob = checkout -b
    d = diff
    f = fetch --all
    heroku = push heroku master
    l = log
    m = merge
    pl = pull
    pop = stash pop
    pu = push
    r = rebase
    s = status
